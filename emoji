#!/bin/sh

copy_emoji() {
    read emoji name aliases
    echo "copying $name to pasteboard"
    printf '%s' "$emoji" | pbcopy
}

main() {
    selecta | copy_emoji
}

# this list generated from https://github.com/Ranks/emoji-alpha-codes

